{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "dialog:default",
    "store:allow-get",
    "store:allow-set",
    "store:allow-has",
    "store:allow-delete",
    "store:allow-save",
    "store:allow-load",
    "store:allow-keys",
    "store:allow-clear",
    "core:window:default",
    "core:window:allow-start-dragging",
    "core:window:allow-close",
    "core:window:allow-minimize",
    "core:window:allow-toggle-maximize",
    "updater:default",
    "process:allow-restart"
  ]
}